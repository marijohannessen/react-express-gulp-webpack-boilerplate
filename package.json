{
  "name": "React-Express-Gulp-Webpack-Boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for creating React + Express + Gulp + Webpack application",
  "main": "app/bin/www",
  "scripts": {
    "clean": "rimraf app/dist",
    "build": "node tools/build --release",
    "start": "node tools/start",
    "postinstall": "npm run build:prod",
    "dev": "gulp",
    "test": "mocha tests",
    "deploy": "node tools/deploy.gh --release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marijohannessen/react-express-gulp-webpack-boilerplate"
  },
  "keywords": [
    "react",
    "movie",
    "list"
  ],
  "author": "Mari Johannessen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marijohannessen/react-express-gulp-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/marijohannessen/react-express-gulp-webpack-boilerplate#readme",
  "dependencies": {
    "babel-core": "6.9.1",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.9.0",
    "babel-runtime": "^6.9.2",
    "browser-sync": "2.13.0",
    "compression": "1.6.2",
    "ejs": "2.4.2",
    "express": "4.13.4",
    "extend": "^3.0.0",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.0",
    "gulp-htmlmin": "2.0.0",
    "gulp-nodemon": "2.1.0",
    "gulp-rename": "1.2.2",
    "gulp-sass": "2.3.1",
    "gulp-sourcemaps": "1.6.0",
    "gulp-util": "3.0.7",
    "helmet": "^2.1.1",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-hot-loader": "^1.3.0",
    "require-dir": "0.3.0",
    "rimraf": "2.5.2",
    "webpack": "^1.13.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-stream": "3.2.0"
  },
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "babel-preset-stage-1": "^6.5.0",
    "cpy": "^4.0.1",
    "eslint": "2.12.0",
    "eslint-plugin-babel": "3.2.0",
    "eslint-plugin-react": "5.1.1",
    "git-repository": "^0.1.4",
    "mocha": "2.5.3",
    "morgan": "1.7.0",
    "re-base": "^1.5.1",
    "react-router": "^2.4.1",
    "supertest": "1.2.0",
    "task": "^0.1.0",
    "webpack-dev-middleware": "^1.6.1"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-1"
    ],
    "plugins": [
      "transform-runtime",
      "transform-regenerator"
    ]
  }
}
